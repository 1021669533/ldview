!IF DEFINED(QTDIR)
all: QT\LDView.exe

QT\LDView.exe:	TRE\$(OBJDIR)\TRE.lib TCFoundation\$(OBJDIR)\TCFoundation.lib
QT\LDView.exe:	LDLib\$(OBJDIR)\LDLib.lib LDLoader\$(OBJDIR)\LDLoader.lib

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: LDView.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) LDView.pro
qmake: 
	@$(QMAKE) LDView.pro

dist: 
	@mkdir -p .obj/LDView && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/LDView/ && ( cd `dirname .obj/LDView` && $(TAR) LDView.tar LDView && $(GZIP) LDView.tar ) && mv `dirname .obj/LDView`/LDView.tar.gz . && rm -rf .obj/LDView

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:
	-rm -f $(UICIMPLS) $(UICDECLS)

clean: mocclean uiclean
	-rm -f $(OBJECTS) 
		-rm -f .ui/qmake_image_collection.cpp
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
